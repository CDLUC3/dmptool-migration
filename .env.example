# Environment Variables for DMP Tool Migration

# Source Database Configuration (Rails/ActiveRecord system)
SOURCE_DB_HOST=localhost
SOURCE_DB_PORT=5432
SOURCE_DB_NAME=dmptool_production
SOURCE_DB_USER=dmptool_user
SOURCE_DB_PASSWORD=your_source_password

# Target Database Configuration (Node Apollo system)
TARGET_DB_HOST=localhost
TARGET_DB_PORT=5432
TARGET_DB_NAME=apollo_production
TARGET_DB_USER=apollo_user
TARGET_DB_PASSWORD=your_target_password

# DuckDB Configuration
DUCKDB_PATH=./dmptool_migration.db

# Migration Configuration
MIGRATION_BATCH_SIZE=1000
MIGRATION_LOG_LEVEL=INFO